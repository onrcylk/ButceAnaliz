@model IEnumerable<ButceAnaliz.Models.Gider>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ElektirikFatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SuFatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DoğalgazFatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InternetFatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TelefonFatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KrediTutar)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ElektirikFatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SuFatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DoğalgazFatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InternetFatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelefonFatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KrediTutar)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
